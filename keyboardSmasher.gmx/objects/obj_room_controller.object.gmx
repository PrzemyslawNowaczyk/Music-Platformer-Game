        <arguments>
          <argument>
            <kind>1</kind>
            <string>if ( music_controller.head_position &lt; 1 ){
    if ( !beats_from_previous_song_already_added ){
        beats_from_previous_song = beats;
        beats_from_previous_song_already_added = true;
        song_beats = 0;
    }
}
else{
    beats_from_previous_song_already_added = false;
}

if ( music_controller.paused == false ){
    if (music_controller.head_position != music_controller.last_known_head_position){
        //tykniecia = _wielkosc kafla_ * BPS * _szybkosc obiektu_ * (pozycja glowicy + przesuniecie tasmy);
        computed_beats = 1 * (music_controller.tempo / 60) * 1 * (music_controller.head_position + music_controller.shift) + beats_from_previous_song;
        beats_speed_factor = 1 - (beats - computed_beats)/5;
    }
}
else{
    beats_speed_factor = 1;
}
/*
beats += music_controller.tempo/60 * (time_controller.frame_time/1000)// * beats_speed_factor;
song_beats += music_controller.tempo/60 * (time_controller.frame_time/1000)// * beats_speed_factor;
bars = floor(beats/4);
*/

beats += music_controller.tempo/60 * 0.0166666 * beats_speed_factor;
song_beats += music_controller.tempo/60 * 0.0166666 * beats_speed_factor;
bars = beats/4;
song_bars = song_beats/4;

//show_debug_message(song_beats);
//beats += 0.02;
</string>
